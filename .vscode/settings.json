{
  "cSpell.words": ["hayagriva", "Hayagriva"],
  "explorer.fileNesting.patterns": {
    "*.schema.json": "${capture}.yaml,${capture}.yml"
  },
  "json.schemas": [
    {
      "fileMatch": ["docs/*.schema.json"],
      "url": "http://json-schema.org/draft-07/schema#"
    }
  ],
  "yaml.schemas": {
    "https://json-schema.org/draft-07/schema#": "docs/*.yaml"
  }
}
